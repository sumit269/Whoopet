<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EditTextView">
        <attr name="labelDrawable" format="reference" />
        <attr name="labelText" format="string|reference" />
        <attr name="customHint" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="shadow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="circularImageViewStyle" format="reference" />
    </declare-styleable>

</resources>